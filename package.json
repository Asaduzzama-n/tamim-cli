{
  "name": "builder",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "build": "tsc && pkg . --targets node18-win-x64 --no-bytecode --output executable/builder.exe",
    "build:watch": "tsc --watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "global": "^4.4.0",
    "mongoose": "^8.8.2",
    "pkg": "^5.8.1",
    "typescript": "^5.7.2"
  },
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "assets": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "outputPath": "executable"
  },
  "bin": "./dist/index.js",
  "devDependencies": {
    "@types/node": "^22.9.3"
  }
}
